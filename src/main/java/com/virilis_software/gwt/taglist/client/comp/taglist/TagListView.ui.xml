<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:vs="urn:import:com.virilis_software.gwt.taglist.client.comp.taglist"
        ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
        ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
        ui:generateLocales='default'>
        
    <ui:with field="css" type="com.virilis_software.gwt.taglist.client.resource.Resources" />
    
    <g:HTMLPanel ui:field="tagListPanel" styleName="{css.style.tagList}">
        <vs:TagsPanel ui:field="tagsPanel">
            <li ui:field="inputListItem" class="{css.style.inputListItem}">
                <g:TextBox ui:field="inputTextBox" stylePrimaryName="{css.style.inputField}" />
            </li>
        </vs:TagsPanel>
        <span ui:field="textHandler" style="float: left; left: -1000px; position: absolute; display: inline-block;" />
    </g:HTMLPanel>
</ui:UiBinder>

